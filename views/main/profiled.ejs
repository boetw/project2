<div class= "col-xs-offset-3">
<h1>Wishlist</h1>
</div>

	<% results.forEach(function(data){ %>
		<% if (data.kit != 1){ %>
		<div class="well col-lg-3 col-md-4 col-sm-4 col-xs-10 col-xs-offset-1 col-md-offset-1 col-lg-offset-0" style="height:70px">
		<h4 style="color:black;">
		<a href="/details/<%=data.set_id%>">
			<img src="./img/fill.jpg"  class="img-responsive" alt="<%= data.theme1 %>" style=" display:inline-block;height: 40px;">
			<%= data.descr%> (<%= data.year %>) </h4>
			</a>
	</div>
	<br>
	<%}%>
	<% }); %>

</div>
