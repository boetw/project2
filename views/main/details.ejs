	<div class="col-xs-10 col-xs-offset-1" style="height:50px;"></div>
	<% details.forEach(function(data){ %>
		<div class="col-lg-6 col-md-6 col-xs-10 col-xs-offset-1 col-md-offset-3">
		<h1 style="color:black;"><%= data.descr%> <small>(<%= data.year %>)</small> </h1>
		<br>
			<img src="https://img.rebrickable.com/img/sets-b/<%=data.set_id.toLowerCase() %>.jpg"  class="img-responsive" alt="<%= data.theme1 %>" style=" display:inline-block;max-height: 200px;">
			<h4>Type: <%= data.theme %><h4>
			<h4>Number of Pieces:<%= data.pieces %><h4>
	</div>
	<br>
	<% }); %>
	<% if (currentUser){%>
		<div class="col-xs-12">
		<input class="btn btn-primary" type="Submit" value="OWN">
		<input class="btn btn-primary" type="submit" value="WANT">
		<div class="btn btn-success">OWNED</div>
		<div class="btn btn-success">ON WISHLIST</div>

		</div>
		<%}%>